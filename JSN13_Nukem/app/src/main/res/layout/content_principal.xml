<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.mobile.jsn.jsn13_nukem.PrincipalActivity"
    tools:showIn="@layout/app_bar_principal">



    <android.support.v7.widget.CardView
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="125dp"
        android:id="@+id/card1"

        >


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="9dp"
            android:elevation="20dp"

            >


            <ImageView
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:background="@color/cardview_dark_background"
                android:id="@+id/imagem1"

                />

            <TextView
                android:id="@+id/cardview_titulo1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"

                android:paddingBottom="5dp"
                android:text="Vivo de Bico Mobilidade"
                android:textSize="24sp" />

            <TextView
                android:id="@+id/cardview_descricao1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/cardview_titulo1"
                android:text="Free android tutorials and examples for beginners and expert."
                android:textColor="#8b8b8b" />

        </RelativeLayout>
    </android.support.v7.widget.CardView>



